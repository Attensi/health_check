# Gemfile for health_test testing

source 'https://rubygems.org'

gem 'rails', "~> 3.0.19"
gem 'rake', '>= 0.8.3', '< 11.0'        # REQUIRED

group :development, :test do
  if defined?(JRUBY_VERSION)
    gem 'jruby-openssl'
    gem 'activerecord-jdbcsqlite3-adapter'
  else
    gem 'sqlite3', "~> 1.3.7"
  end
  gem 'shoulda', "~> 2.11.0"
end
